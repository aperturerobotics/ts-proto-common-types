{
  "name": "@aperturerobotics/ts-proto-common-types",
  "version": "0.2.1",
  "description": "Common types for ts-proto.",
  "license": "MIT",
  "files": [
    "!**/*.tsbuildinfo",
    "Makefile",
    "README.md",
    "go.mod",
    "go.sum",
    "google",
    "patches",
    "tsconfig.build.json",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "check": "npm run typecheck",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write './!(vendor|dist)/**/(*.ts|*.tsx|*.html|*.css)'",
    "prepare": "patch-package",
    "precommit": "npm run format",
    "gen": "make genproto"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "patch-package": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-proto": "^1.117.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "long": "^5.2.0",
    "protobufjs": "^7.0.0"
  }
}
